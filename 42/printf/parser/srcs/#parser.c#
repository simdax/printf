static int	is_flag()
{
  int i;

  i = 0;
  while (*str)
    {
      if (*str == '#' || *str == '0' ||
  	  *str == ' ' || *str == '+' ||
  	  *str == '-')
	{i++}
      else
	return (i);
      str++;
    }
}



