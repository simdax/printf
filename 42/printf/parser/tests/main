#!/bin/bash

tests=(	"0+#-' "       		"111111000"
	" "			"000001000"
	"#0 ##" 		"101001000"
	"0+-ri" 		"110100000"	
	"0+-10.10" 		"110100000"
	"0+-010.10" 		"110100000"
	"0+-01.2d" 		"11010012d"
	"0+-010.10D" 		"1101001010D"
	"0+-010.1060osdfgx" 	"110100101060o"
	"0+-0q" 		"11010000q"
)

function cmp
{
	eval $red; echo "GROS TEST $3" 
	if [ -n $3 ] ; then
	    diff <(eval $1) <(echo -n $2)
	fi
}

for i in $(seq 0 2 $((${#tests[@]} - 1))); do
	cmp "../parser \"${tests[$i]}\"" ${tests[$(($i + 1))]} $i
done

