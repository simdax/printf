
#include "chained_list.h"

t_list *sort(char **chaine)
{
  t_list	*head;
  t_list	*tmp;
  int val;

  val = ft_atoi(*chaine);
  head = ft_lstnew(&val, sizeof(int));
  chaine++;
  tmp = head;
  while (*chaine)
    {
      val = ft_atoi(*chaine);
      tmp->next = ft_lstnew(&val, sizeof(int));
      tmp = tmp->next;
      chaine++;
    }
  return (head);
}

int main(int argc, char**argv)
{
  if (argc == 1)
    {
      return (1);
    }
  argv++;
  sort(argv);
  return (0);
}

